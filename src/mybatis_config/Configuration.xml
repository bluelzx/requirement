			alias="taskAlarm" />	
		<typeAlias type="com.lanzx.rmmsys.entity.ManhaurList"
			alias="manhaurList" />
		<typeAlias type="com.lanzx.rmmsys.entity.ManhaurListSum"
			alias="manhaurListSum" />	
		<typeAlias type="com.lanzx.rmmsys.entity.ProjectTrack"
			alias="projectTrack" />	
	</typeAliases>
	
	
	<mappers>
		<mapper resource="./mybatis_config/SysUser.xml"/>
		<mapper resource="./mybatis_config/SysMenu.xml"/>
		<mapper resource="./mybatis_config/SysRequirement.xml"/>
		<mapper resource="./mybatis_config/SysTask.xml"/>
		<mapper resource="./mybatis_config/File.xml"/>
		<mapper resource="./mybatis_config/Log.xml"/>
