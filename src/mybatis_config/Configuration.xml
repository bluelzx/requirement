			alias="taskDependable" />
		<typeAlias type="com.lanzx.rmmsys.entity.Log"
			alias="log" />
		<typeAlias type="com.lanzx.rmmsys.entity.Parameter"
			alias="para" />
		<typeAlias type="com.lanzx.rmmsys.entity.TaskHaurs"
			alias="taskHaurs" />
		<typeAlias type="com.lanzx.rmmsys.entity.RequirementCount"
			alias="RequirementCount" />
		<typeAlias type="com.lanzx.rmmsys.entity.TaskRCounts"
			alias="TaskRCounts" />
		<typeAlias type="com.lanzx.rmmsys.entity.TaskSignIn"
			alias="TaskSignIn" />
		<typeAlias type="com.lanzx.rmmsys.entity.TodayTaskSignIn"
			alias="TodayTaskSignIn" />
		<typeAlias type="com.lanzx.rmmsys.entity.Vacation"
			alias="Vacation" />
